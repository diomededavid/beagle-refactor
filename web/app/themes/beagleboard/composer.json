{
  "name": "roots/sage",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme with a modern development workflow",
  "homepage": "https://roots.io/sage/",
  "authors": [
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    },
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "QWp6t",
      "email": "hi@qwp6t.me",
      "homepage": "https://github.com/qwp6t"
    },
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me",
      "homepage": "https://github.com/log1x"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/roots/sage/issues",
    "forum": "https://discourse.roots.io/"
  },
  "repositories": {
    "0": {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    },
    "acf-pro": {
      "type": "composer",
      "url":"https://connect.advancedcustomfields.com"
    },
    "my-yoast": {
      "type": "composer",
      "url": "https://my.yoast.com/packages/",
      "only": [
        "yoast/wordpress-seo-premium",
        "yoast/wordpress-seo-local",
        "yoast/wpseo-video",
        "yoast/wpseo-woocommerce",
        "yoast/yoast-news-seo"
      ]
    },
    "gravityforms":{
      "type": "package",
      "package": {
        "name": "gravityforms/gravityforms",
        "version": "2.9.16",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://www.gravityhelp.com/wp-content/plugins/gravitymanager/api.php?op=get_plugin&slug=gravityforms&key=9131c9c32b8bd675a358196ec9e0f5e2"
        },
        "require": {
          "composer/installers": "^1.4",
          "gotoandplay/gravityforms-composer-installer": "^2.3"
        }
      }
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "wpengine/advanced-custom-fields-pro": "^6.5.0.1",
    "ffraenz/private-composer-installer": "^5.0",
    "gravityforms/gravityforms": "^2.9.16",
    "johnbillion/extended-cpts": "^4.5",
    "roots/acorn": "*",
    "roots/soil": "^4.0",
    "wpackagist-plugin/all-bootstrap-blocks": "^1.3.30",
    "wpackagist-plugin/classic-editor": "^1.6",
    "wpackagist-plugin/code-syntax-block": "^3.1.1",
    "wpackagist-plugin/download-monitor": "^4.9.14",
    "wpackagist-plugin/featured-images-for-rss-feeds": "^1.6.2",
    "wpackagist-plugin/font-awesome": "^5.1.0",
    "wpackagist-plugin/generateblocks": "^ 2.1.0",
    "wpackagist-plugin/safe-svg": "^2.3.3",
    "wpackagist-plugin/w3-total-cache": "^2.8.12",
    "wpackagist-plugin/wordpress-importer": "^0.8.1",
    "wpackagist-plugin/wp-discourse": "^2.4",
    "wpackagist-plugin/wp-fail2ban": "^5.2.1",
    "wpackagist-plugin/wp-fail2ban-addon-gravity-forms": "^2.0",
    "wpackagist-plugin/wp-search-with-algolia": "^2.10.3",
    "wpackagist-plugin/youtube-embed-plus": "^14.0",
    "yoast/wordpress-seo": "^25.7",
    "yoast/wordpress-seo-premium": "^23.1"
  },
  "require-dev": {
    "laravel/pint": "^1.20"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "ffraenz/private-composer-installer": true,
      "gotoandplay/gravityforms-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
